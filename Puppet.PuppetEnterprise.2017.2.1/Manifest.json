    {
        "$schema": "https://gallery.azure.com/schemas/2015-10-01/manifest.json#",
        "name": "PuppetEnterprise",
        "publisher": "Puppet",
        "version": "2017.2.1",
        "displayName": "ms-resource:displayName",
        "publisherDisplayName": "ms-resource:publisherDisplayName",
        "publisherLegalName": "ms-resource:publisherLegalName",
        "summary": "ms-resource:summary",
        "longSummary": "ms-resource:longSummary",
        "description": "ms-resource:description",
        "uiDefinition": {
            "path": "UIDefinition.json"
        },
        "artifacts": [
            {
                "name": "createuidefinition",
                "type": "Custom",
                "path": "DeploymentTemplates\\createUiDefinition.json",
                "isDefault": false
            },
            {
                "name": "DefaultTemplate",
                "type": "Template",
                "path": "DeploymentTemplates\\mainTemplate.json",
                "isDefault": true
            },
            {
                "name": "azuredeploy.parameters.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\azuredeploy.parameters.json",
                "isDefault": false
            },
            {
                "name": "compute-password.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\compute-password.json",
                "isDefault": false
            },
            {
                "name": "compute-sshPublicKey.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\compute-sshPublicKey.json",
                "isDefault": false
            },
            {
                "name": "publicip-existing.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\publicip-existing.json",
                "isDefault": false
            },
            {
                "name": "publicip-new.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\publicip-new.json",
                "isDefault": false
            },
            {
                "name": "storage-existing.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\storage-existing.json",
                "isDefault": false
            },
            {
                "name": "storage-new.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\storage-new.json",
                "isDefault": false
            },
            {
                "name": "NetworkSecurityGroup.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\networkSecurityGroup.json",
                "isDefault": false
            },
            {
                "name": "vnet-existing.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\vnet-existing.json",
                "isDefault": false
            },
            {
                "name": "vnet-new.json",
                "type": "Custom",
                "path": "DeploymentTemplates\\vnet-new.json",
                "isDefault": false
            }
        ],
        "categories": [
            "Compute"
        ],
        "links": [
            { "displayName": "ms-resource:documentationLink", "uri": "https://docs.puppet.com/pe/latest/index.html" },
            { "displayName": "ms-resource:documentationLink2", "uri": "https://docs.puppet.com/pe/latest/pe_architecture_overview.html" }
        ],
        "images": [
            {
                "context": "ibiza",
                "items": [
                    {
                        "id": "small",
                        "path": "icons\\Small.png",
                        "type": "icon"
                    },
                    {
                        "id": "medium",
                        "path": "icons\\Medium.png",
                        "type": "icon"
                    },
                    {
                        "id": "large",
                        "path": "icons\\Large.png",
                        "type": "icon"
                    },
                    {
                        "id": "wide",
                        "path": "icons\\Wide.png",
                        "type": "icon"
                    },
                    {
                        "id": "screenShot0",
                        "path": "screenshots\\Screenshot1.png",
                        "type": "screenshot"
                    }
                ]
            }
        ]
    }